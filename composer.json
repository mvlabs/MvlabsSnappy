{
    "name": "drigani/mva-snappy",
    "description": "MvaSnappy is a ZF2 module that allow easy to thumbnail, snapshot or PDF generation from a url or a html page using Snappy PHP (5.3+) wrapper for the  wkhtmltopdf/wkhtmltoimage conversion utility.",
    "license": "MIT",
    "type": "zf2-module",
    "keywords": [
        "snappy",
        "wkhtmltopdf",
        "wkhtmltoimage", 
        "pdf", 
        "thumbnail", 
        "snapshot", 
        "MvaAssociati", 
        "MvLabs", 
        "zf2"
    ],
    "homepage": "https://github.com/mvassociati/MvaSnappy.git",
    "authors": [
        {
            "name": "Diego Drigani",
            "email": "d.drigani@mvassociati.it",
            "homepage": "http://www.mvassociati.it/en/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "knplabs/knp-snappy": "0.1.0"
    },    
    "autoload": {
        "psr-0": {
            "MvaSnappy": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
